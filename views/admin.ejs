	<body class="admin">
		<main class="admin-container">
			<article class="admin-screens">
				<article class="admin-screen admin-screen__loader admin-screen--active">
					<div class="spinner"></div>
					<h1 class="loading">
						<%= loadingText %>
					</h1>
				</article>
				<section class="admin-screen admin-screen__admin">
					<hgroup id="showRoom">
						<h1>rumskod</h1>
						<h2></h2>
					</hgroup>

					<h1 class="mainHeader">Hjälpkö</h1>
